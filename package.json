{
  "name": "ts-boilerplate",
  "version": "0.0.1",
  "description": "Minimal boilerplate for typescript projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npx webpack build",
    "serve": "tsc && npx webpack build && npx webpack serve",
    "test": "mocha --exit --require ts-node/register test/*.ts",
    "bin": "npm run build && node dist/bin/index.js"
  },
  "bin": {
    "ts-boilerplate": "dist/bin/index.js"
  },
  "keywords": [
    "ts",
    "typescript",
    "boilerplate",
    "sample",
    "example",
    "javascript"
  ],
  "author": "Ivan Pusic",
  "license": "MIT",
  "dependencies": {
    "commander": "8.3.0",
    "webpack-cli": "^4.9.1"
  },
  "devDependencies": {
    "@types/chai": "4.3.0",
    "@types/commander": "2.12.2",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.12",
    "chai": "^4.3.4",
    "mocha": "9.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "10.4.0",
    "tslint": "5.20.1",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-dev-server": "^4.6.0"
  }
}
